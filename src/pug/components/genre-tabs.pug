.pn-ProductNav_Wrapper
    nav#pnProductNav.pn-ProductNav
        #pnProductNavContents.pn-ProductNav_Contents
            a(href='#' aria-selected="true" data-key="all").pn-ProductNav_Link All
            a(href='#' data-key="music").pn-ProductNav_Link MUSIC
            a(href='#' data-key="dance").pn-ProductNav_Link DANCE
            a(href='#' data-key="theatre").pn-ProductNav_Link THEATRE
            a(href='#' data-key="visualarts").pn-ProductNav_Link VISUAL ARTS
            a(href='#' data-key="literaryarts").pn-ProductNav_Link LITERARY ARTS
            span#pnIndicator.pn-ProductNav_Indicator
    button(type="button")#pnAdvancerLeft.pn-Advancer.pn-Advancer_Left.px-3.bg-white
        img(src="./assets/microsites/offstage/img/icons/Previous.svg")

    button(type="button")#pnAdvancerRight.pn-Advancer.pn-Advancer_Right.px-3.bg-white
        img(src="./assets/microsites/offstage/img/icons/Next.svg")
